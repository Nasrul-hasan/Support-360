    <validationRules>
        <fullName>Project_Mandatory_Fields_Check</fullName>
        <active>true</active>
        <description>This validation rule ensures that when creating a Project record, all mandatory fields are completed. It checks that the Project Name, Start Date, End Date, and Priority are provided and that a Customer Account is assigned.</description>
        <errorConditionFormula>OR(
  ISBLANK(Name),
  ISBLANK(Start_Date__c),
  ISBLANK(End_Date__c),
  ISPICKVAL(Priority__c, &quot;&quot;),
  ISBLANK(Account__c)
)</errorConditionFormula>
        <errorMessage>Please complete all required fields: Project Name, Start Date, End Date, Priority, and assign a Customer Account</errorMessage>
    </validationRules>
